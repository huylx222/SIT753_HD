{
    "title": "CPU Usage by Pod",
    "xyChart": {
      "dataSets": [
        {
          "timeSeriesQuery": {
            "timeSeriesFilter": {
              "filter": "metric.type=\"kubernetes.io/container/cpu/core_usage_time\" resource.type=\"k8s_container\" resource.labels.container_name=\"api\"",
              "aggregation": {
                "perSeriesAligner": "ALIGN_RATE",
                "crossSeriesReducer": "REDUCE_SUM",
                "groupByFields": [
                  "resource.label.pod_name"
                ]
              }
            },
            "unitOverride": "1"
          },
          "plotType": "LINE",
          "targetAxis": "Y1"
        },
        {
          "timeSeriesQuery": {
            "timeSeriesFilter": {
              "filter": "metric.type=\"kubernetes.io/container/cpu/core_usage_time\" resource.type=\"k8s_container\" resource.labels.container_name=\"web\"",
              "aggregation": {
                "perSeriesAligner": "ALIGN_RATE",
                "crossSeriesReducer": "REDUCE_SUM",
                "groupByFields": [
                  "resource.label.pod_name"
                ]
              }
            },
            "unitOverride": "1"
          },
          "plotType": "LINE",
          "targetAxis": "Y1"
        }
      ],
      "yAxis": {
        "label": "CPU usage (cores)",
        "scale": "LINEAR"
      }
    }
  }
  